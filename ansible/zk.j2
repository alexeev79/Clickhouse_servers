<yandex>
  <zookeeper>
{% for server in zk %}
        <node index="{{ loop.index }}">
     {% for key, value in server.items() %}
           <{{ key }}>{{ value }}</{{ key }}>
     {% endfor %}
        </node>
{% endfor %}
  </zookeeper>
  <networks>
      <ip>::/0</ip>
  </networks>
  <clickhouse_compression>
      <case>
           <min_part_size>10000000000</min_part_size>
           <min_part_size_ratio>0.01</min_part_size_ratio>
           <method>lz4</method>
      </case>
  </clickhouse_compression>
</yandex>

